{
  "id": null,
  "title": "Keycloak Metrics",
  "tags": ["keycloak", "monitoring"],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "graph",
      "title": "Total HTTP Requests",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "http_server_requests_seconds_count{job=\"keycloak\"}",
          "legendFormat": "{{uri}}",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 6 }
    },
    {
      "type": "graph",
      "title": "HTTP Error Rate (4xx/5xx)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(http_server_requests_seconds_count{job=\"keycloak\", status=~\"4..|5..\"}[1m])",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 6 }
    },
    {
      "type": "graph",
      "title": "Request Duration (p95)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{job=\"keycloak\"}[1m])) by (le, uri))",
          "legendFormat": "p95 {{uri}}",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 6, "w": 12, "h": 6 }
    },
    {
      "type": "graph",
      "title": "Active Sessions",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "keycloak_sessions_active{job=\"keycloak\"}",
          "legendFormat": "{{realm}}",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 12, "y": 6, "w": 12, "h": 6 }
    },
    {
      "type": "graph",
      "title": "Failed Login Attempts",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "keycloak_login_failures_total{job=\"keycloak\"}",
          "legendFormat": "{{realm}}",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 12, "w": 12, "h": 6 }
    },
    {
      "type": "graph",
      "title": "Successful Logins",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "keycloak_logins_total{job=\"keycloak\"}",
          "legendFormat": "{{realm}}",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 12, "y": 12, "w": 12, "h": 6 }
    }
  ]
}
